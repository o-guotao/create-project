{
  "name": "gt-hooks",
  "version": "1.0.0",
  "type": "module",
  "description": "gt React业务 自定义hooks库",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./lib/index.d.ts",
  "unpkg": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    }
  },
  "keywords": [
    "react",
    "hooks",
    "custom-hooks",
    "gt"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf lib dist es",
    "test": "cross-env NODE_ENV=test vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "build": "rollup -c"
  },
  "author": "GrowthCoder",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/mockjs": "^1.0.10",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cross-env": "^10.1.0",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "mockjs": "^1.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}